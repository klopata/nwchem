start lih_rhf_sto-3g_fci_1.680

echo

geometry units angstrom
symmetry c1
Li   0.000000000000000   0.000000000000000   0.000000000000000
H    0.000000000000000   0.000000000000000   1.680000000000000
end

basis
H library sto-3g
Li library sto-3g
end

scf
thresh 1.0e-10
tol2e 1.0e-10
singlet
rhf
end

tce
ccsd
tilesize 1
2eorb
2emet 13
nroots 5
thresh 1.0e-6
end

set tce:print_integrals T
set tce:qorb 6
set tce:qela 2
set tce:qelb 2

task tce energy

mcscf
  active  6
  actelec 4
  multiplicity 1
end

task mcscf

